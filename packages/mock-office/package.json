{
  "name": "mock-office",
  "version": "1.1.0",
  "description": "App for mocking API",
  "main": "dist/bin/mock-office.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "echo 'skipping mock-office build: temporary disabled'",
    "watch": "babel src --out-dir dist --source-maps --watch --ignore src/lib/gui/",
    "serve": "pm2 start ecosystem.config.js",
    "dev": "npm-run-all clean build watch",
    "build-gui": "webpack --progress",
    "watch-gui": "webpack-dev-server",
    "test-gui": "jest src/gui",
    "tdd-gui": "jest src/gui --watch",
    "watch-all": "npm-run-all --parallel watch-gui watch-app",
    "log": "tail -F pm2.log",
    "log-errors": "tail -F pm2-errors.log",
    "start": "node ./dist/bin/mock-office.js",
    "run": "npm-run-all build start",
    "lint": "",
    "tdd": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "watch": {
    "build-main": {
      "patterns": [
        "src/main/**/*.js",
        "!src/**/*.test.js"
      ]
    }
  },
  "repository": "https://bitbucket.org/clixr/mock-office",
  "keywords": [
    "mock api"
  ],
  "author": "Bartosz Adamczyk <bartoszadamczykk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.24.4",
    "coveralls": "^3.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-environment-jsdom-11.0.0": "^20.1.9",
    "jest-fetch-mock": "^1.1.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.2.1",
    "prettier": "^1.13.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "request": "^2.87.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "ajv": "^4.11.6",
    "atob": "^2.1.1",
    "body-parser": "^1.17.1",
    "btoa": "^1.1.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "command-line-args": "^4.0.2",
    "commander": "^2.15.0",
    "cors": "^2.8.3",
    "cuid": "^1.3.8",
    "deep-equal": "^1.0.1",
    "enzyme-to-json": "^1.4.5",
    "esm": "^3.0.6",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "fun-task": "^1.5.2",
    "http-proxy": "^1.16.2",
    "http-proxy-middleware": "^0.18.0",
    "json8": "^0.9.2",
    "modalo": "^0.2.3",
    "node-http-proxy": "^0.2.3",
    "npm-watch": "^0.1.8",
    "prop-types": "^15.5.7",
    "ramda": "^0.25.0",
    "ramda-fantasy": "^0.7.0",
    "rc-switch": "^1.4.4",
    "react": "^15.4.1",
    "react-custom-scrollbars": "^4.0.2",
    "react-dom": "^15.4.1",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.4",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-observable": "^0.13.0",
    "reselect": "^2.5.4",
    "rxjs": "5",
    "source-map-support": "^0.4.11",
    "whatwg-fetch": "^2.0.3",
    "ws": "^3.3.1",
    "xync": "^0.1.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|svg)$": "<rootDir>/jest/webpackAssetsMock.js",
      "\\.(css|scss)$": "<rootDir>/jest/stylesMock.js"
    },
    "setupFiles": [
      "./jest/fetchMock.js",
      "./jest/rafMock.js"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test/unit"
    ],
    "testEnvironment": "jest-environment-jsdom-11.0.0"
  },
  "bin": {
    "mock-office": "./dist/bin/mock-office.js"
  }
}
